citEntry(
  entry    = "Unpublished",
  title    = "CompHeatmaps: Easy visualization of raw 16S reads for metatranscriptomic analysis",
  author   = "Koji Wong",
  year     = 2023,
  note     = "Unpublished",
  textVersion =
  paste("Wong, K. (2023) CompHeatmaps: Easy visualization of raw 16S reads for metatranscriptomic analysis.
        Unpublished. URL https://github.com/kojiwong/CompHeatmaps.")
)
